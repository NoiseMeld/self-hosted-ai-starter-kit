POSTGRES_USER=root
POSTGRES_PASSWORD=password
POSTGRES_DB=n8n

N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret
N8N_DEFAULT_BINARY_DATA_MODE=filesystem

# n8n Owner/Admin Setup
# Update these with your details for the initial admin account
N8N_OWNER_EMAIL=your-email@example.com
N8N_OWNER_PASSWORD=YourSecurePassword123!

# For Mac users running OLLAMA locally
# See https://github.com/NoiseMeld/self-hosted-ai-starter-kit?tab=readme-ov-file#for-mac--apple-silicon-users
# OLLAMA_HOST=host.docker.internal:11434

# Cloudflare Tunnel Token (optional)
# Get this token by running: cloudflared tunnel token your-tunnel-name
# Only needed if using the cloudflare profile for global access
# TUNNEL_TOKEN=your-tunnel-token-here


